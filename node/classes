typedef struct _node {
    int canary1;
    char* val;
    node* left;
    node* right;
    int checksum;

}

node:: node(.../char* val_) {
    canary1 = canary;
    canary2 = canary;
    left = 0;
    right = 0;
    val =" "/val;
    checksum = Checksum();
}

node::node():canary1(canary),left(0),right(0),...

node* temp = new node;
node* tmp = new node[10];
delete temp;
delete []tmp;
